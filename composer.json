{
  "name": "fahl-design/log4php-slack-appender",
  "description": "A log4php appender build as wrapper for maknz/slack package",
  "keywords": ["log", "logging", "php", "slack"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Fahl",
      "email": "ben@webproject.xyz",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">= 5.2.7 || ^7.0",
    "roave/security-advisories": "dev-master",
    "apache/log4php": "^2.3",
    "maknz/slack": "1.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "sllh/php-cs-fixer-styleci-bridge": "^1.5"
  },
  "autoload": {
    "classmap": ["src/main/php/"]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
