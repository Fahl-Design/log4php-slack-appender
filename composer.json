{
  "name": "fahl-design/log4php-slack-appender",
  "description": "A log4php appender build as wrapper for maknz/slack package",
  "keywords": ["log", "logging", "php", "slack"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Fahl",
      "email": "ben@webproject.xyz",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">= 5.5 || >=7",
    "roave/security-advisories": "dev-master",
    "apache/log4php": "^2.3",
    "maknz/slack": "1.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.* || 5.6.*",
    "phpunit/php-invoker": "1.*",
    "mockery/mockery": "0.9.*",
    "sllh/php-cs-fixer-styleci-bridge": "2.1.*",
    "friendsofphp/php-cs-fixer": "1.*",
    "codeclimate/php-test-reporter": "^0.3.2"
  },
  "autoload": {
    "classmap": ["src/main/php/"]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
